<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission-sdk-23 android:name="android.permission.INTERNET"></uses-permission-sdk-23>

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Nimbus"
        tools:targetApi="31">
        <activity
            android:name=".PlayerInfomationScreen"
            android:exported="true"
            android:label="@string/title_activity_player_infomation_screen"
            android:theme="@style/Theme.Nimbus" />
        <activity
            android:name=".EventsScreen"
            android:exported="true"
            android:label="@string/title_activity_events_screen"
            android:theme="@style/Theme.Nimbus" />
        <activity
            android:name=".RosterScreen"
            android:exported="true"
            android:label="@string/title_activity_roster_screen"
            android:theme="@style/Theme.Nimbus" />
        <activity
            android:name=".DashboardScreen"
            android:exported="true"
            android:label="@string/title_activity_dashboard_screen"
            android:theme="@style/Theme.Nimbus" />
        <activity
            android:name=".MyTeamsScreen"
            android:exported="true"
            android:label="@string/title_activity_my_teams_screen"
            android:theme="@style/Theme.Nimbus" />
        <activity
            android:name=".LoginScreen"
            android:exported="true"
            android:label="@string/title_activity_login_screen"
            android:theme="@style/Theme.Nimbus" />
        <activity
            android:name=".Register2Screen"
            android:exported="true"
            android:label="@string/title_activity_register2_screen"
            android:theme="@style/Theme.Nimbus" />
        <activity
            android:name=".SplashScreen"
            android:exported="true"
            android:label="Nimbus"
            android:theme="@style/Theme.Nimbus">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".OnboardingScreen"
            android:exported="true"
            android:label="@string/title_activity_onboarding_screen"
            android:theme="@style/Theme.Nimbus" />
        <activity
            android:name=".Register1Screen"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Nimbus" />
    </application>

</manifest>